// Teste focado na Geracao de TAC para Classes e Objetos

class Box {
  init(valorInicial) {
    this.conteudo = valorInicial;
  }

  getConteudo() {
    return this.conteudo;
  }

  setConteudo(novoValor) {
    this.conteudo = novoValor;
  }
}

var caixa = Box(100);

print caixa.getConteudo();

caixa.setConteudo(200);

caixa.conteudo = 300;
print caixa.conteudo;